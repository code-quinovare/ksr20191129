.sjinfonav {
    height: 11vh;
}

.nav-two {
    height: 11vh;
}

.toatal_head {
    height: 11vh;
    line-height: normal;
}

.toataltime {
    position: absolute;
    top: 0;
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.toataltime>text:nth-child(1) {
    font-size: 26rpx;
}

.toataltime>text:nth-child(2) {
    margin-top: 10rpx;
    font-size: 24rpx;
}

.yxrq {
    padding: 20rpx 30rpx;
    color: #666;
    font-size: 26rpx;
}

.timearr {
    padding: 0 30rpx 20rpx;
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: flex-start;
    margin-bottom: 120rpx;
}

.timecell {
    width: 200rpx;
    padding: 40rpx 60rpx;
    margin: 0 15rpx 20rpx;
    border-radius: 5rpx;
    background: #fff;
}