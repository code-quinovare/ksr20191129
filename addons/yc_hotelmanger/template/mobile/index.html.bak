          // 百度地图API功能                var geoc = new BMap.Geocoder();                var pt = new BMap.Point(lng, lat);                geoc.getLocation(pt, function (rs) {                    var addComp = rs.addressComponents;        //            var address = addComp.province + addComp.city + addComp.district + addComp.street + addComp.streetNumber;                    var address = addComp.province + addComp.city                    $('#near').attr("href","{php echo $this->createMobileUrl('index')}"+"&cityName="+addComp.city);                   // alert($("#near").attr("href"));                    val = address;                    //对地址操作                    $('#locala').html(address);                    // alert(address)                    // return address;                });            }            function handleError(error) {}                    </script>    <script>        $(function () {            mui('.pop-schwrap .sch-input').input();            var deceleration = mui.os.ios?0.003:0.0009;            mui('.pop-schwrap .scroll-wrap').scroll({                bounce: true,                indicators: true,                deceleration:deceleration            });            $('.top-sch-box .sch-txt,.pop-schwrap .btn-back').on('click',function () {                $('html,body').toggleClass('holding');                $('.pop-schwrap').toggleClass('on');                if($('.pop-schwrap').hasClass('on')) {;                    $('.pop-schwrap .sch-input').focus();                }            });            $('.top-sch-box .fdj,.top-sch-box .sch-txt,.pop-schwrap .btn-back').on('click',function () {                $('html,body').toggleClass('holding');               var hotelName =  $('#hotelName').val();                if(hotelName) {                    window.location.href="{php echo $this->createMobileUrl('index')}"+"&hotelName="+hotelName;                }else{                   // $('.pop-schwrap .sch-in