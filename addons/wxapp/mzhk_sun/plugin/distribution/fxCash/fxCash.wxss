.block {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0rpx 20rpx;
    height: 150rpx;
    background: #f5ac32;
    color: #fff;
}

.cash {
    margin-top: 16rpx;
    line-height: 42rpx;
    font-size: 30rpx;
}

.cash>text {
    font-size: 36rpx;
    font-weight: 700;
}

.detail {
    padding: 5rpx 20rpx;
    height: 52rpx;
    line-height: 52rpx;
    border: 1rpx solid #fff;
    border-radius: 4rpx;
    word-break: 2rpx;
}

.session {
    padding-left: 20rpx;
    margin-top: 20rpx;
    background: #fff;
}

.list {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding-right: 20rpx;
    height: 90rpx;
    border-bottom: 1rpx solid #ececec;
}

.list:last-child {
    border-bottom: 0rpx solid #ececec;
}

.list>view:last-child {
    color: #f5ac32;
    font-size: 30rpx;
}

.btn>button {
    margin-top: 80rpx;
    width: 650rpx;
    height: 90rpx;
    line-break: 90rpx;
    font-size: 32rpx;
    color: #fff;
    background: #f5ac32;
}