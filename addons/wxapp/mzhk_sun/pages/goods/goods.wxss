.goods-search {
    position: fixed;
    top: 0rpx;
    left: 0rpx;
    overflow: hidden;
    width: 750rpx;
    height: 70rpx;
    background: #fff;
    z-index: 99;
}

.goods-search>view {
    display: flex;
    align-items: center;
    margin: 10rpx 50rpx;
    width: 650rpx;
    background: #f8f8f8;
    height: 50rpx;
    border-radius: 10rpx;
}

._search {
    display: flex;
    align-items: center;
    transform: translateX(200rpx);
    transition: all 0.2s ease-out;
    height: 70rpx;
}

.showSearch {
    transform: translateX(0rpx);
}

.searchIcon {
    height: 28rpx;
    width: 28rpx;
    margin-right: 20rpx;
}

#mz {
    margin-top: 70rpx;
    padding: 30rpx 50rpx;
    background: #fff;
}

.mz-card {
    background: #000;
    color: #f5ac32;
    position: relative;
    height: 380rpx;
    width: 650rpx;
    border-radius: 30rpx;
    box-shadow: 0rpx 2rpx 21rpx rgba(0,0,0,0.5);
    background-size: cover;
}

.mz-logo {
    width: 600rpx;
    position: absolute;
    top: 50rpx;
    left: 50%;
    margin-left: -300rpx;
}

.mz-logo ._mzLogo {
    display: flex;
    flex-direction: column;
    align-items: center;
    font-size: 42rpx;
    color: #f5ac32;
    font-weight: 700;
}

._mzLogo text {
    display: block;
    width: 600rpx;
    overflow: hidden;
    height: 46rpx;
    line-height: 46rpx;
    text-align: center;
}

.hklogo {
    width: 80rpx;
    height: 80rpx;
    margin-bottom: 30rpx;
    border-radius: 50%;
}

.uthumb {
    width: 82rpx;
    height: 82rpx;
    border-radius: 50%;
    overflow: hidden;
    display: block;
}

.mz-right {
    text-align: center;
}

.mz-right text {
    display: inline-block;
    margin-bottom: 20rpx;
}

.rightNow {
    width: 190rpx;
    height: 70rpx;
    border-radius: 12rpx;
    line-height: 70rpx;
    text-align: center;
    background: #412d2e;
    font-size: 30rpx;
    border: 1rpx solid #f5ac32;
}

.mz-cont {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 246rpx 30rpx 0rpx;
}

.mz-awater {
    display: flex;
    line-height: 40rpx;
}

.mz-pic {
    border: 2rpx solid #f5ac32;
    width: 82rpx;
    height: 82rpx;
    border-radius: 50%;
    margin-right: 20rpx;
}

.opear {
    display: flex;
    padding: 24rpx 15rpx 0rpx;
    background: #fff;
    margin-top: 20rpx;
    flex-wrap: wrap;
}

.operaIcon {
    height: 72rpx;
    width: 72rpx;
    margin-bottom: 20rpx;
}

.opearList {
    width: 180rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    padding-bottom: 30rpx;
    font-size: 24rpx;
}

.order-nav {
    height: 118rpx;
    display: flex;
    align-items: center;
    background: #fff;
    padding: 0rpx 30rpx;
    margin-top: 20rpx;
}

.order-nav view {
    display: inline-block;
    height: 118rpx;
    line-height: 118rpx;
    box-sizing: border-box;
    color: #666;
    font-size: 30rpx;
    margin-right: 50rpx;
}

.order-nav .on {
    border-bottom: 4rpx solid #f5ac32;
    color: #f5ac32;
}

.order-nav text {
    padding: 0rpx 10rpx;
}

.session {
    background: #faf9f9;
    padding: 35rpx 30rpx 30rpx;
}

.list {
    margin-bottom: 30rpx;
    box-shadow: 0rpx 0rpx 24rpx 0rpx rgba(3,3,3,0.086);
    border-radius: 12rpx;
    border: 1rpx solid #f5e0be;
    padding: 24rpx 30rpx 0rpx;
    width: 630rpx;
}

.list-up {
    display: flex;
    justify-content: flex-start;
    padding-bottom: 30rpx;
}

.list-down {
    display: flex;
    justify-content: space-between;
    border-top: 1rpx solid #ececec;
    padding-bottom: 20rpx;
}

.list-up image {
    width: 180rpx;
    height: 180rpx;
    border-radius: 12rpx;
}

.goods-det {
    margin-left: 28rpx;
    margin-top: 10rpx;
    position: relative;
}

.lst {
    padding: 0rpx 30rpx 0rpx;
}

.list-title {
    font-weight: 700;
    font-size: 30rpx;
    margin-bottom: 10rpx;
    line-height: 36rpx;
}

.det {
    margin-bottom: 6rpx;
    color: #666;
}

.det text {
    margin-right: 30rpx;
    font-size: 24rpx;
}

.tj {
    display: flex;
    align-items: center;
    color: #666;
    font-size: 26rpx;
}

.list-up .good {
    width: 32rpx;
    height: 30rpx;
    margin-right: 10rpx;
}

.discount {
    display: flex;
    margin-top: 30rpx;
}

.icon {
    width: 36rpx;
    height: 36rpx;
    background: rgba(255,0,0,0.2);
    font-size: 22rpx;
    color: #f33030;
    line-height: 36rpx;
    text-align: center;
    margin-right: 20rpx;
}

.youhui {
    height: 36rpx;
    line-height: 36rpx;
    font-weight: 700;
}

.dc {
    color: #f33030;
    margin-top: 14rpx;
}

.down {
    width: 124rpx;
    text-align: center;
    font-size: 24rpx;
    color: #666;
    margin-top: 4rpx;
}

.receive,.receive-have {
    height: 60rpx;
    width: 124rpx;
    background: #f33030;
    border-radius: 30rpx;
    color: #fff;
    text-align: center;
    line-height: 60rpx;
    margin-top: 16rpx;
    font-size: 26rpx;
    margin-bottom: 24rpx;
}

.receive-have {
    background: #ddd;
    color: #000;
}

.vip {
    width: 36rpx;
    height: 36rpx;
    line-height: 36rpx;
    text-align: center;
    border-radius: 6rpx;
    font-size: 24rpx;
    margin-right: 20rpx;
}

.vip,.vip image {
    width: 36rpx;
    height: 36rpx;
}

.member {
    border-top: 1rpx solid #dcdcdc;
    padding: 0rpx 30rpx;
    height: 88rpx;
    background: #fff;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.member text {
    color: #ef8200;
    font-weight: 700;
    max-width: 233rpx;
    height: 88rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.member>view {
    color: #ef8200;
    font-weight: 700;
}

.swiper {
    height: 88rpx;
    width: 560rpx;
    line-height: 88rpx;
}

.swiper swiper-item {
    display: flex;
    align-items: center;
}

.celebrate {
    width: 50rpx;
    height: 50rpx;
    margin-right: 20rpx;
    border-radius: 50%;
}

.mask {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1000;
    background: #000;
    opacity: 0.5;
    overflow: hidden;
}

.d_box {
    width: 600rpx;
    height: 660rpx;
    position: fixed;
    top: 50%;
    left: 50%;
    margin-top: -330rpx;
    margin-left: -300rpx;
    z-index: 1001;
}

.d_cont {
    padding: 50rpx 50rpx;
    background: #fff;
    font-size: 30rpx;
    position: absolute;
    top: 50%;
    transform: translate(0%,-50%);
    line-height: 40rpx;
    color: #666;
    width: 85%;
}

.d_title {
    padding-bottom: 20rpx;
    font-size: 32rpx;
    color: #f33030;
    text-align: center;
}

.apply {
    background: #f33030;
    height: 80rpx;
    width: 100%;
    border-radius: 12rpx;
    line-height: 80rpx;
    text-align: center;
    color: #fff;
    margin-top: 30rpx;
}

.d_content {
    max-height: 385rpx;
    overflow-y: auto;
}

.search-list {
    position: fixed;
    top: 70rpx;
    bottom: 98rpx;
    left: 0rpx;
    right: 0rpx;
    background: #fff;
    z-index: 9999;
    overflow-y: scroll;
    overflow-x: hidden;
}

.show-search-list {
    animation: showSearch 0.5s linear;
}

@keyframes showSearch {
    0% {
        opacity: 0;
        transform: translateX(-750rpx);
    }

    100% {
        opacity: 1;
        transform: translateX(0rpx);
    }
}

.search-close {
    padding: 20rpx 30rpx 0rpx 30rpx;
    text-align: right;
}

.search-close>image {
    height: 34rpx;
    width: 34rpx;
}

.seach-item {
    display: flex;
    margin: 0rpx 30rpx 0 30rpx;
    padding: 20rpx 0rpx;
    border-bottom: 1rpx solid #ececec;
}

.seach-item:last-child {
    border-bottom: 0rpx solid #ececec;
}

.search-item-left>image {
    width: 200rpx;
    height: 113rpx;
}

.search-item-right {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    margin-left: 30rpx;
}

.search-goods-title {
    line-height: 36rpx;
}

.search-goods-price {
    color: #f5ac32;
    font-size: 26rpx;
    font-weight: 700;
}

.searchkeyword {
    padding: 10rpx 30rpx;
    display: flex;
    flex-wrap: wrap;
}

.searchkeyword text {
    margin-right: 30rpx;
    padding: 10rpx 15rpx;
    background: #ececec;
    border-radius: 5rpx;
    margin-bottom: 10rpx;
}