@import "../../wxParse/wxParse.wxss";

.header {
    position: relative;
    height: 522rpx;
}

.watch {
    position: absolute;
    z-index: 1001;
    background: rgba(0,0,0,0.5);
    width: 710rpx;
    height: 52rpx;
    padding: 0rpx 20rpx;
    top: 0rpx;
    left: 0rpx;
    color: #fff;
    font-size: 24rpx;
    text-align: right;
    display: flex;
    align-items: center;
    justify-content: flex-end;
}

.watch>view {
    border-left: 1rpx solid #fff;
    padding: 0rpx 12rpx;
    height: 34rpx;
    display: flex;
    align-items: center;
}

.watch>view:first-child {
    border-left: 0rpx solid #fff;
}

.watch text {
    display: inline-block;
    font-size: 24rpx;
    line-height: 24rpx;
    padding: 4rpx 6rpx;
    border: 1rpx solid #fff;
    border-radius: 6rpx;
    margin-right: 6rpx;
}

.page {
    position: absolute;
    right: 20rpx;
    bottom: 30rpx;
    height: 36rpx;
    width: 90rpx;
    background: rgba(0,0,0,0.5);
    color: #fff;
    font-size: 24rpx;
    text-align: center;
    line-height: 36rpx;
    border-radius: 18rpx;
}

swiper {
    height: 525rpx;
}

.slide-image {
    width: 750rpx;
    height: 525rpx;
}

.block {
    position: relative;
    height: 100rpx;
}

.timeLimit {
    position: absolute;
    top: 0rpx;
    left: 0rpx;
    height: 52rpx;
    padding: 0rpx 20rpx;
    width: 710rpx;
    color: #fff;
    line-height: 52rpx;
    text-align: center;
    font-size: 24rpx;
    display: flex;
    justify-content: space-between;
    background: rgba(0,0,0,0.5);
}

.timeLimit>text {
    color: #fff;
    z-index: 1003;
}

.block-cont {
    width: 710rpx;
    height: 100rpx;
    position: absolute;
    top: 0rpx;
    background-image: -webkit-linear-gradient(0deg,rgb(255,80,16)0%,rgb(255,139,12)100%);
    display: flex;
    justify-content: space-between;
    padding: 0rpx 20rpx;
    align-items: center;
    color: #fff;
}

.block-cont-price {
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.block-cont-price>view:nth-child(1) {
    padding-right: 10rpx;
    border-right: 1px dashed #ccc;
}

.block-cont-price>view:nth-child(2) {
    padding-left: 10rpx;
}

.block-cont-price>view:nth-child(2) view:nth-child(1) {
    padding-bottom: 15rpx;
    padding-left: 10rpx;
}

.block-cont-price>view:nth-child(2) view:nth-child(1) text {
    background: #222;
    color: #fffb08;
    margin-bottom: 5rpx;
    padding: 2rpx 10rpx;
    font-size: 24rpx;
}

.price {
    font-size: 42rpx;
}

.price view {
    overflow: inherit;
    font-size: 42rpx;
}

.price text {
    font-size: 26rpx;
}

.oldprice {
    position: relative;
    margin-top: 10rpx;
}

.oldprice::before {
    content: " ";
    display: inline-block;
    width: 100%;
    height: 2rpx;
    background: #fff;
    position: absolute;
    top: 20rpx;
    transform: rotate(20deg,20deg);
}

.oldprice text {
    font-size: 26rpx;
}

.wrap {
    border-bottom: 1rpx solid #cbcbcb;
    background: #fff;
}

.wrap_up {
    margin-left: 30rpx;
    padding-right: 30rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 130rpx;
    overflow: hidden;
}

.wrap_down {
    color: #999;
    margin-left: 20rpx;
    height: 100rpx;
    line-height: 100rpx;
}

.wrap_time {
    color: #222;
}

.wrap_up:first-child {
    border-bottom: 1rpx solid #ececec;
}

.block-down {
    display: flex;
    justify-content: space-between;
    padding-right: 30rpx;
    align-items: center;
    height: 120rpx;
    color: #222;
    border-bottom: 1rpx solid #ececec;
    padding-left: 20rpx;
}

.addr {
    display: flex;
    align-items: center;
    line-height: 36rpx;
}

.phone {
    width: 58rpx;
    height: 58rpx;
}

.shopName {
    margin-bottom: 10rpx;
    line-height: 30rpx;
}

.addrIcon {
    width: 30rpx;
    height: 32rpx;
    margin-right: 20rpx;
}

.title {
    font-size: 34rpx;
    font-weight: 700;
    line-height: 46rpx;
    width: 580rpx;
}

.share {
    margin-left: 0rpx;
    margin-right: 0rpx;
    padding-left: 0rpx;
    padding-right: 0rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 20rpx;
    width: 80rpx;
    height: 80rpx;
    border-radius: 50%;
    background: #f2f2f2;
    color: #999;
    line-height: 28rpx;
}

.share::after {
    border: none;
}

.share image {
    width: 30rpx;
    height: 30rpx;
}

.spec {
    padding: 0rpx 30rpx;
    display: flex;
    flex-wrap: wrap;
    padding-bottom: 30rpx;
    color: #999;
}

.specList {
    display: flex;
    align-items: center;
    width: 230rpx;
    margin-top: 30rpx;
}

.specList image {
    width: 30rpx;
    height: 30rpx;
    margin-right: 12rpx;
}

.shopspec {
    background: #fff;
    color: #222;
}

.process {
    display: flex;
    flex-direction: row;
    padding: 30rpx 20rpx;
    margin-top: 20rpx;
    align-items: cneter;
    background: #fff;
}

.pro {
    width: 194rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    color: #222;
}

.proimg {
    width: 56rpx;
    height: 56rpx;
    margin-bottom: 16rpx;
}

.arrow {
    width: 56rpx;
    height: 16rpx;
    margin-top: 8rpx;
}

.order-nav {
    margin-top: 20rpx;
    height: 84rpx;
    display: flex;
    align-items: center;
    background: #fff;
    justify-content: space-around;
}

.order-nav view {
    height: 100%;
    width: 375rpx;
    text-align: center;
    line-height: 84rpx;
    box-sizing: border-box;
}

.order-nav .on {
}

.order-nav .on text {
    color: #ef8200;
}

.session {
    padding: 20rpx 0rpx 150rpx;
}

.goods-det {
    padding: 20rpx;
    background: #fff;
    word-break: break-all;
}

.goods-det view {
    line-height: 45rpx;
}

.session-title {
    display: flex;
    justify-content: center;
    align-items: center;
    padding: 35rpx 0rpx 35rpx;
    font-size: 30rpx;
}

.session-title:after,.session-title:before {
    content: " ";
    display: inline-block;
    width: 62rpx;
    height: 2rpx;
    background: #cbcbcb;
}

.session-title text {
    color: #666;
    padding: 0rpx 24rpx;
}

.guessList {
    padding: 0rpx 20rpx;
    display: flex;
    flex-wrap: wrap;
    justify-content: flex-start;
}

.gus-item {
    width: 333rpx;
    padding: 5rpx;
    border: 1rpx solid #dcdcdc;
    margin-bottom: 20rpx;
}

.gus-item:nth-child(odd) {
    margin-right: 20rpx;
}

.gus-img {
    width: 333rpx;
    height: 342rpx;
}

.gus-title {
    margin-top: 20rpx;
    font-size: 30rpx;
}

.gus-price {
    display: flex;
    justify-content: space-between;
    color: #f33030;
    margin-top: 10rpx;
    padding-bottom: 5rpx;
    line-height: 42rpx;
}

.mIcon {
    font-size: 32rpx;
}

.gusPrice {
    font-size: 42rpx;
}

.old {
    margin-top: 4rpx;
    color: #666;
}

.old::before {
    background: #666;
}

.session-comm {
    padding-bottom: 150rpx;
    height: 500rpx;
}

.comm-cont {
    display: flex;
    justify-content: space-between;
    padding: 20rpx 20rpx;
    background: #fff;
    margin-bottom: 12rpx;
}

.uthumb {
    width: 76rpx;
    height: 76rpx;
    border-radius: 50%;
}

.comm-left {
    width: 76rpx;
    height: 76rpx;
    margin-right: 14rpx;
    border-radius: 50%;
}

.comm-up {
    height: 76rpx;
    line-height: 76rpx;
    display: flex;
    justify-content: space-between;
}

.pubtime {
    color: #999;
    font-size: 24rpx;
}

.comment {
    color: #666;
    line-height: 36rpx;
    margin-top: 4rpx;
}

.good {
    display: flex;
    align-items: center;
    justify-content: flex-end;
}

.good text {
    font-size: 26rpx;
    color: #999;
}

.good-img {
    width: 30rpx;
    height: 30rpx;
}

.navFix {
    display: flex;
    justify-content: space-between;
    height: 98rpx;
    width: 750rpx;
    position: fixed;
    bottom: 0rpx;
    background: #ffe7e3;
}

.navFix .tobuy {
    width: 632rpx;
    height: 98rpx;
    background-image: -webkit-linear-gradient(0deg,rgb(255,134,0)0%,rgb(255,105,14)100%);
    line-height: 98rpx;
    color: #fff;
    text-align: center;
    font-size: 30rpx;
}

.navFix .over {
    background-image: -webkit-linear-gradient(0deg,rgb(202,202,202)0%,rgb(154,154,154)100%);
}

.navFix .toIndex {
    height: 98rpx;
    width: 118rpx;
    background: #fff;
    color: #999;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 20rpx;
    line-height: 26rpx;
}

.toIndex image {
    width: 60rpx;
    height: 60rpx;
}

.navFix .toGroup {
    color: #fff;
    background: #f42;
}

.countDown {
    font-size: 24rpx;
    text-align: center;
    line-height: 36rpx;
}

.countDown text {
    display: inline-block;
    background: #fff;
    height: 28rpx;
    padding: 0rpx 6rpx;
    color: #ff5110;
    text-align: center;
    line-height: 28rpx;
    margin: 0rpx 5rpx;
    border-radius: 2rpx;
}

.preview {
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.7);
    position: fixed;
    z-index: 1001;
    top: 0rpx;
}

.preview image {
    width: 80%;
    position: absolute;
    top: 10%;
    left: 10%;
    z-index: 9999;
}

.preview button {
    width: 40%;
    position: absolute;
    bottom: 100rpx;
    left: 30%;
    z-index: 9999;
}

.canvas {
    position: absolute;
    top: -3000rpx;
}

.shareBtn {
    position: absolute;
    top: 100rpx;
    right: 30rpx;
    z-index: 1001;
    width: 60rpx;
    height: 60rpx;
    background: rgba(0,0,0,0.7);
    border-radius: 50%;
    color: #fff;
    font-size: 22rpx;
    display: flex;
    align-items: center;
    padding: 20rpx;
    justify-content: center;
    text-align: center;
    line-height: 34rpx;
}

.shareBtn>text {
    display: inline-block;
}

.pro_wg {
    display: flex;
}

.pro_wg .pro_wg_logo {
    margin-right: 20rpx;
}

.pro_wg .pro_wg_logo,.pro_wg .pro_wg_add {
    width: 80rpx;
    height: 80rpx;
}

.pro_wg .pro_wg_title {
    width: 535rpx;
    line-height: 37rpx;
    overflow: hidden;
}

.pro_wg .pro_wg_title view {
    height: 37rpx;
    overflow: hidden;
}

.pro_wg .pro_wg_title view:nth-child(2) {
    color: #bbb;
}

.codemask {
    position: absolute;
    top: 0rpx;
    left: 0rpx;
    right: 0rpx;
    bottom: 0rpx;
    background: rgba(0,0,0,0.5);
    z-index: 1000;
}

.code {
    position: absolute;
    top: 35%;
    left: 50%;
    transform: translate(-50%);
    z-index: 1001;
    background: #fff;
    width: 500rpx;
    min-height: 250rpx;
    padding-bottom: 30rpx;
    border-radius: 10rpx;
    color: #ef8200;
}

.codetitle {
    text-align: center;
    padding: 56rpx 0 30rpx;
}

.codecontent {
    padding: 0 20rpx;
    text-align: center;
    line-height: 35rpx;
}

.qrcode {
    text-align: center;
    margin-top: 50rpx;
    margin-bottom: 20rpx;
}

.qrcode button {
    background: #ef8200;
    color: #fff;
    text-align: center;
    padding: 0rpx 15rpx;
    width: 175rpx;
    height: 50rpx;
    line-height: 48rpx;
    font-size: 30rpx;
}