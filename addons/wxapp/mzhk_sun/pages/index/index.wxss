    color: #fff;
    left: 0rpx;
    border-radius: 0rpx 20rpx 20rpx 0rpx;
    top: 80rpx;
}

.recoUp>view.shopname {
    top: 30rpx;
    max-width: 260rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.recoDown {
    padding: 16rpx 20rpx;
}

.recoDown>view {
    width: 295rpx;
    font-size: 24rpx;
    color: #666;
}

.recoDown>view.goodsTitle {
    display: -webkit-box!important;
    text-overflow: ellipsis;
    word-break: break-all;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    height: 70rpx;
    overflow: hidden;
    font-size: 28rpx;
    margin-bottom: 16rpx;
    color: #222;
    font-weight: 700;
    line-height: 34rpx;
    letter-spacing: 2rpx;
}

.recoUp>view.activeTips {
    top: 0rpx;
    border-radius: 10rpx 0rpx 20rpx;
}

.sessionGroup {
    margin-top: 20rpx;
    padding-top: 20rpx;
}

.recoDown>view.shopName {
    color: #f5ac32;
}

.recoDown>view.shopPrice {
    color: #f5ac32;
    font-size: 28rpx;
    font-weight: 700;
}

.welfarScroll {
