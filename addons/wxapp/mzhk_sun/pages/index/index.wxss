    margin-right: 20rpx;
}

.list {
    background: #fff;
    margin-bottom: 30rpx;
    box-shadow: 0rpx 0rpx 24rpx 0rpx rgba(3,3,3,0.086);
}

.list-up {
    width: 690rpx;
    height: 390rpx;
