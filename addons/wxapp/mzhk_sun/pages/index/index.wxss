    height: 70rpx;
    border-radius: 12rpx;
    line-height: 70rpx;
    text-align: center;
    background: #412d2e;
    font-size: 30rpx;
    border: 1rpx solid #f5ac32;
}

.mz-cont {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 246rpx 30rpx 0rpx;
}

.mz-awater {
