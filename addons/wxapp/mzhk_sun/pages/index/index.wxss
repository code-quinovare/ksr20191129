}

.list-time {
    color: #999;
    font-size: 22rpx;
    font-weight: 700;
}

.list-time text {
    margin-right: 10rpx;
}

.btn {
    width: 154rpx;
    height: 62rpx;
    background: #f5ac32;
    border-radius: 10rpx;
    text-align: center;
    line-height: 62rpx;
    color: #fff;
}

.brands {
    display: inline-block;
    padding: 0rpx 50rpx 0rpx 30rpx;
    margin-top: 20rpx;
    background: #f5ac32;
    color: #fff;
    min-width: 200rpx;
    height: 50rpx;
    line-height: 50rpx;
}

.lsts {
    padding: 0rpx 30rpx 0rpx;
}

.lists-title {
    margin-bottom: 20rpx;
    margin-top: 16rpx;
}

.lists-time {
    color: #999;
    font-size: 24rpx;
    display: flex;
    justify-content: space-between;
    padding-bottom: 32rpx;
}

.lists-time text {
    margin-right: 10rpx;
}

.lists-time .num {
    color: #f5ac32;
}

.lst {
    flex: 1;
}

.lst .list-title>text:last-child {
    width: 400rpx;
    line-height: 32rpx;
    max-height: 64rpx;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
}

.adMask {
    position: fixed;
    top: 0rpx;
    bottom: 0rpx;
    left: 0rpx;
    right: 0rpx;
    background: rgba(0,0,0,0.5);
    z-index: 1000;
}

.ads {
    z-index: 1001;
    position: fixed;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
}

.adIcon {
    width: 54rpx;
    height: 54rpx;
    position: absolute;
    right: 0rpx;
    top: -80rpx;
}

.adimg {
    max-width: 710rpx;
    min-width: 340rpx;
    width: 650rpx;
    max-height: 750rpx;
    height: 400rpx;
}

.adSwiper {
    z-index: 1003;
    max-height: 750rpx;
    width: 650rpx;
    height: 400rpx;
}

.plate_form {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
}

.plate_form button {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    background: none;
}

.plate_form button::after {
    border: none;
}

.free {
    position: fixed;
    bottom: 170rpx;
    right: 30rpx;
    z-index: 999;
}

.free button {
    background: none;
    padding: 0;
    margin: 0;
    line-height: auto;
    font-size: 0px;
}

.free button:after {
    border: 0px;
}

.free image {
    width: 90rpx;
    height: 90rpx;
}

.toOther {
    bottom: 280rpx;
}

.theme_two swiper {
    height: 285rpx;
    margin-top: 0px;
}

.ad2 {
    background: #fff;
    padding: 0rpx 30rpx;
}

.ad2 image {
    width: 690rpx;
    height: 240rpx;
    border-radius: 20rpx;
    box-shadow: 0rpx 2rpx 21rpx rgba(0,0,0,0.5);
}

.bargain {
    width: 690rpx;
    white-space: nowrap;
    padding-bottom: 15rpx;
    height: 480rpx;
    position: relative;
}

.barList {
    display: inline-block;
    border-radius: 10rpx 10rpx 10rpx 10rpx;
    width: 320rpx;
    margin-right: 20rpx;
    box-shadow: 0 2rpx 21rpx 0 rgba(180,180,180,0.2);
    position: relative;
}

.barList>image {
    width: 320rpx;
    height: 320rpx;
    border-radius: 10rpx 10rpx 0rpx 0rpx;
}

.barList>view {
    margin: 0rpx 20rpx;
    overflow: hidden;
}

.barPrice {
    font-size: 28rpx;
    padding-top: 20rpx;
    line-height: 36rpx;
    font-weight: 700;
    white-space: normal;
    display: -webkit-box!important;
    width: 280rpx;
    height: 72rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    word-break: break-all;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.barOldPrice {
    font-size: 26rpx;
    color: #f5ac32;
    display: inline-block;
    position: relative;
    padding-top: 15rpx;
    padding-bottom: 10rpx;
    font-weight: 700;
}

.barOldPrice>text:last-child {
    margin-left: 20rpx;
    text-decoration: line-through;
    font-size: 24rpx;
    color: #999;
}

.recommand {
    display: flex;
    flex-wrap: wrap;
}

.recoList {
    display: inline-block;
    width: 335rpx;
    margin-bottom: 30rpx;
    border-radius: 20rpx 20rpx 20rpx 20rpx;
    box-shadow: 0 2rpx 21rpx 0 rgba(180,180,180,0.2);
}

.recoList:nth-child(odd) {
    margin-right: 20rpx;
}

.recoUp {
    position: relative;
    border-radius: 20rpx 20rpx 0rpx 0rpx;
    width: 335rpx;
}

.recoUp>image {
    width: 335rpx;
