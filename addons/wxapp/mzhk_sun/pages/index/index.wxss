@import "psPage/psPage.wxss";

swiper {
    height: 340rpx;
    margin-top: 20rpx;
}

.slide-image {
    width: 750rpx;
    height: 340rpx;
}

.userOpe {
    background-color: #fff;
    padding: 34rpx 20rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
}

.userOpe_auto {
    background-color: #fff;
    padding: 24rpx 10rpx;
    display: flex;
    flex-wrap: wrap;
}

.userOpe_auto .opeartion {
    margin: 10rpx 10rpx;
    flex: 1;
}

.userOpe_auto swiper-item {
    display: flex;
    padding: 0 0 -20rpx;
}

.opeartion {
    display: flex;
    flex-direction: column;
    align-items: center;
    position: relative;
}

.opeartion image {
    width: 52rpx;
    height: 52rpx;
}

.opeartion text {
    font-size: 28rpx;
    color: #222;
    margin-top: 20rpx;
}

._ope text {
    display: block;
}

.opera-swiper {
    height: 180rpx;
    width: 100%;
    margin-top: 0;
}

.goods-search {
    position: fixed;
    top: 0rpx;
    left: 0rpx;
    overflow: hidden;
    width: 750rpx;
    height: 70rpx;
    background: #fff;
    z-index: 99;
}

.goods-search>view {
    display: flex;
    align-items: center;
    margin: 10rpx 50rpx;
    width: 650rpx;
    background: #f8f8f8;
    height: 50rpx;
    border-radius: 10rpx;
}

._search {
    display: flex;
    align-items: center;
    transform: translateX(200rpx);
    transition: all 0.2s ease-out;
    height: 70rpx;
}

.showSearch {
    transform: translateX(0rpx);
}

.showSearch input {
    width: 200rem;
}

.searchIcon {
    height: 28rpx;
    width: 28rpx;
    margin-right: 20rpx;
}

#mz {
    margin-top: 70rpx;
    padding: 30rpx 50rpx;
    background: #fff;
}

