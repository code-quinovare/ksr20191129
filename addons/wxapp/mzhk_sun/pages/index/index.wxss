    font-weight: 700;
    background: rgba(245,172,50,0.5);
    color: #fff;
    overflow: hidden;
}

.list-down {
    height: 122rpx;
    display: flex;
    justify-content: space-between;
    padding: 24rpx 20rpx 0rpx;
}

.list-title {
    font-weight: 700;
    margin-bottom: 20rpx;
    display: flex;
    align-items: center;
}

.list-title>text {
    display: inline-block;
}

.list-title .vip {
    height: 32rpx;
    line-height: 32rpx;
