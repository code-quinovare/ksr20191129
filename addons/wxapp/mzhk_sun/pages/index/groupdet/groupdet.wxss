    padding-left: 20rpx;
    background: #fff;
    margin-top: 20rpx;
}

.addr {
    display: flex;
    align-items: center;
    line-height: 36rpx;
}

.phone {
    width: 58rpx;
    height: 58rpx;
}

.shopName {
    margin-bottom: 10rpx;
}

.addrIcon {
    width: 30rpx;
    height: 32rpx;
    margin-right: 20rpx;
}

.preview {
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.7);
    position: fixed;
    z-index: 1001;
    top: 0rpx;
}

.preview image {
    width: 80%;
    position: absolute;
    top: 10%;
    left: 10%;
    z-index: 9999;
}

.preview button {
    width: 40%;
