.card-prices {
    padding: 6rpx 20rpx 20rpx;
    display: flex;
    justify-content: space-between;
    align-items: cneter;
    border: 1rpx solid rgb(40,183,163);
    color: #28b7a3;
    border-bottom: 1rpx dashed rgb(40,183,163);
    border-radius: 12rpx 12rpx 0rpx 0rpx;
    position: relative;
}

.card-prices:before,.card-prices:after {
    content: " ";
    display: block;
    width: 16rpx;
    height: 16rpx;
    background: #f8f8f8;
    border-radius: 50%;
    position: absolute;
    bottom: -12rpx;
    border: 1rpx solid rgb(40,183,163);
    z-index: 99;
    border-left: 1rpx solid transparent;
    border-bottom: 1rpx solid transparent;
}

.card-prices:before {
    left: -10rpx;
    transform: rotate(45deg);
}

.card-prices:after {
    transform: rotate(-135deg);
    right: -10rpx;
}

.card-time {
    border: 1rpx solid rgb(40,183,163);
