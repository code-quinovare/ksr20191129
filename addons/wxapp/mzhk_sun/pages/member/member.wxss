.block {
    margin-top: 30rpx;
    background: #fff;
    padding-bottom: 30rpx;
}

.blockTitle {
    font-size: 32rpx;
    height: 82rpx;
    text-align: center;
    line-height: 82rpx;
    font-weight: 700;
    position: relative;
    display: flex;
    justify-content: center;
    align-items: center;
}

.blockTitle>text {
    margin: 0rpx 16rpx;
}

.blockTitle::before,.blockTitle::after {
    content: " ";
    width: 88rpx;
    height: 2rpx;
    background-image: -webkit-linear-gradient(180deg,rgb(239,130,0)0%,rgb(255,255,255)100%);
}

.blockTitle::after {
    background-image: -webkit-linear-gradient(0deg,rgb(239,130,0)0%,rgb(255,255,255)100%);
}

