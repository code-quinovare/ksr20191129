.cash {
    background: #FFF;
    padding: 30rpx 30rpx;
    color: #666;
    margin-top: 20rpx;
}

.enterMoney {
    display: flex;
    align-items: center;
    font-size: 48rpx;
    font-weight: 700;
    margin: 40rpx 0rpx 30rpx;
    color: #222;
    padding-bottom: 20rpx;
    border-bottom: 1rpx solid #ececec;
}

.cashAll {
    display: flex;
    justify-content: space-between;
}

.cashAll>text:last-child {
    display: inline-block;
    color: #ef8200;
}

.submit {
    margin-top: 60rpx;
    padding: 0rpx 30rpx;
}

.submit text {
    display: inline-block;
    color: #ef8200;
}

.submit button {
    margin-top: 30rpx;
    background: #ef8200;
    color: #fff;
    font-size: 32rpx;
    width: 600rpx;
}

.choose {
    margin-top: 20rpx;
    background: #fff;
}

.picker {
    display: flex;
    justify-content: space-between;
    padding-right: 30rpx;
    height: 88rpx;
    align-items: center;
    margin-left: 30rpx;
    width: 690rpx;
}

.picker>view {
    display: flex;
    align-items: center;
}

.picker>view text {
    color: #ef8200;
    font-weight: 700;
}

.right {
    margin-left: 16rpx;
}

.session {
    background: #fff;
    padding-left: 30rpx;
}

.list>view {
    padding: 10rpx 30rpx 10rpx 0rpx;
    border-top: 1rpx solid #ececec;
}

.list input {
    height: 68rpx;
}

.mask {
    background: rgba(0,0,0,0.5);
    position: absolute;
    top: 0rpx;
    left: 0rpx;
    bottom: 0rpx;
    right: 0rpx;
    z-index: 1000;
}

.ruleTitle {
    color: #f33030;
    font-weight: 700;
    font-size: 32rpx;
    text-align: center;
    margin-bottom: 20rpx;
    line-height: 38rpx;
}

.rule {
    background: #fff;
    border-radius: 16rpx;
    width: 550rpx;
    padding: 30rpx;
    position: absolute;
    z-index: 1001;
    top: 40%;
    left: 50%;
    transform: translate(-50%,-70%);
}

.rule>rich-text {
    line-height: 42rpx;
    color: #666;
}