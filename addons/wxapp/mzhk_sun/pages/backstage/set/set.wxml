<view class="container">
    <view>
        <view class="tab">
            <view class="tabnav">
                <view bindtap="toIndex" class="tablist">
                    <image class="tabimg" src="../../../../style/images/tab4.png"></image>
                    <text>工作台</text>
                </view>
                <view bindtap="toMessage" class="tablist">
                    <image class="tabimg" src="../../../../style/images/tab6.png"></image>
                    <text>消息</text>
                </view>
                <view class="tablist  active">
                    <image class="tabimg" src="../../../../style/images/tab1.png"></image>
                    <text>设置</text>
                </view>
            </view>
        </view>
        <view class="session shop">
            <view>
                <text>店铺开店关店</text>
                <text class="status">{{sets[0].status?'店铺已关闭':'店铺已开启'}}</text>
            </view>
            <view>
                <switch bindchange="switchChange" data-index="0"></switch>
            </view>
        </view>
        <view class="session">
            <view>
                <text>订单语音播报</text>
                <text class="status">{{sets[1].status?'语音播报已关闭':'语音播报已开启'}}</text>
            </view>
            <view>
                <switch bindchange="switchChange" data-index="1"></switch>
            </view>
        </view>
    </view>
</view>
